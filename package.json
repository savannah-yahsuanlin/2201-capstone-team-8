    "react-dom": "^16.14.0",
    "react-redux": "^7.0.1",
    "react-router-dom": "^5.0.0",
    "react-test-renderer": "^16.4.2",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-thunk": "^2.3.0",
    "style-loader": "^3.3.1",
    "supertest": "^4.0.2",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1"
  }
